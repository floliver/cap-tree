{
	"import": {
		"as": "cds",
		"force": true,
		"include_namespaces": "sap,c4c"
	},
	"log": {
		"[development]": {
			"levels": {
				"app": "debug",
				"cds": "info"
			},
			"[local]": {
				"levels": {
					"app": "debug",
					"cds": "info"
				}
			},
			"[production]": {
				"levels": {
					"app": "debug",
					"cds": "info"
				}
			}
		}
	},
	"sql": {
		"native_hana_associations": false
	},
	"features": {
		"preserve_computed": true,
		"kibana_formatter": true,
		"audit_personal_data": true,
		"optimized_search": true,
		"rest_new_adapter": true,
		"assert_integrity": "db",
		"odata_new_parser": true
	},
	"fiori": {
		"lean_draft": true,
		"draft_messages": false,
		"direct_crud": true
	},
	"requires": {
		"html5-repo": true,
		"connectivity": true,
		"destinations": true,
		"[production]": {
			"auth": "xsuaa",
			"db": "hana",
			"multitenancy": true
		},
		"[hybrid]": {
			"auth": "mocked",
			"db": "sqlite",
			"multitenancy": false
		},
		"[development]": {
			"auth": {
				"kind": "mocked",
				"users": {
					"admin": {
						"password": "",
						"roles": ["DisassemblyManager"],
						"attr": {
							"PlantGroup": ["MPG2", "MPG", "MPGT", "MPGT2", "GC", "GA"]
						}
					}
				}
			}
		},
		"Plant": {
			"kind": "odata",
			"model": "srv/external/Plant",
