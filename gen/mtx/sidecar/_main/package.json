{
	"name": "cap-tree-issue",
	"version": "1.0.0",
	"description": "A simple CAP project.",
	"repository": "https://github.com/msggroup-Softproviding/cap-tree-issue",
	"license": "UNLICENSED",
	"private": true,
	"engines": {
		"node": ">=22"
	},
	"dependencies": {
		"@cap-js/hana": "^2.5.1",
		"@sap-cloud-sdk/connectivity": "^4.3.1",
		"@sap-cloud-sdk/http-client": "^4.3.1",
		"@sap-cloud-sdk/resilience": "^4.3.1",
		"@cap-js/cds-types": "^0.15.0",
		"@sap/cds": "^9.6.2",
		"@sap/cds-common-content": "^3.1.0",
		"@sap/cds-mtxs": "^3.6.1",
		"@sap/xssec": "^4.11.0",
		"cds-swagger-ui-express": "^0.10.0"
	},
	"devDependencies": {
		"@cap-js/cds-test": "^0.4.1",
		"@cap-js/cds-typer": ">=0.38",
		"@cap-js/sqlite": "^2.1.2",
		"@msggroup-softproviding/eslint-plugin": "^1.1.0",
		"@sap/cds-dk": "^9.6.1",
		"@sap/eslint-plugin-cds": "^4",
		"@types/chai": "^4.3.20",
		"@types/chai-as-promised": "^8.0.2",
		"@types/chai-subset": "^1.3.6",
		"@types/jest": "^30.0.0",
		"@types/mocha": "^10.0.10",
		"@types/node": "^24.10.1",
		"@types/openui5": "^1.142.0",
		"cds-plugin-ui5": "^0.16.1",
		"eslint": "^9.39.1",
		"eslint-plugin-jest": "^29.1.0",
		"eslint-plugin-mocha": "^11.2.0",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jiti": "^2.6.1",
		"prettier": "^3.6.2",
		"ts-jest": "^29.4.5",
		"ts-node": "^10.9.2",
		"tsx": "^4",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.41.0"
	},
	"scripts": {
		"start": "cds-serve",
		"watch": "cds-ts w",
		"clean": "rimraf resources mta_archives gen node_modules @cds-models coverage",
		"build": "rimraf resources mta_archives && mbt build --mtar archive",
		"build:cds": "cds build --production --profile with-mtx-sidecar",
		"build:copycds": "cp gen/srv/@cds-models gen/",
		"local-ts": "cds deploy --to sqlite && cds-ts watch --profile local",
		"hybrid-ts": "cds deploy --to sqlite && cds-ts watch --profile hybrid",
		"local-ce": "cds deploy --to sqlite && cds watch --profile local",
		"local-pce": "cds deploy --to sqlite && cds watch --profile local-private-cloud",
		"dev-ts:inmemory": "cds-ts watch --with-mocks --in-memory",
		"dev-ts": "cds deploy --to sqlite && cds-ts watch --with-mocks",
		"test": "npm run create-types && jest --coverage --silent",
		"watch-managecutlists": "cds watch --open test.disassembly.managecutlists/index.html?sap-ui-xx-viewCache=false --livereload false",
		"create-types": "npx @cap-js/cds-typer \"*\" --outputDirectory @cds-models --useEntitiesProxy true --logLevel WARN",
		"test:jest": "npm run create-types && jest --coverage --silent",
		"test:cicd": "npm run test:jest",
		"prepare": "husky || true",
		"lint": "eslint --cache --fix"
	},
	"sapux": [
		"app/managecutlists"
	],
	"imports": {
		"#cds-models/*": "./@cds-models/*/index.js"
	},
	"lint-staged": {
		"*.{js,ts}": [
			"eslint --cache --fix",
			"prettier --write"
		]
	},
	"cds": {
		"requires": {
			"[production]": {
				"auth": "xsuaa",
				"db": "hana",
				"multitenancy": true
			},
			"[development]": {
				"auth": {
					"kind": "basic",
					"users": {
						"operator": {
							"password": "operator",
							"roles": [
								"OperatorRole"
							]
						},
						"admin": {
							"password": "admin",
							"roles": [
								"ManagerRole"
							]
						}
					}
				}
			},
			"[with-mtx]": {
				"multitenancy": true
			},
			"toggles": true,
			"extensibility": true
		},
		"sql": {
			"native_hana_associations": false
		},
		"profile": "with-mtx-sidecar"
	},
	"type": "module"
}
