COLUMN TABLE test_disassembly_CutListHeaders (
  ID NVARCHAR(36) NOT NULL,
  createdAt TIMESTAMP,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP,
  modifiedBy NVARCHAR(255),
  identifier NVARCHAR(5000),
  description NVARCHAR(5000),
  validFrom DATE NOT NULL DEFAULT CURRENT_TIMESTAMP,
  validTo DATE DEFAULT '9999-12-31',
  material_Product NVARCHAR(18),
  plantGroup_ID NVARCHAR(36),
  calculationQuantity DECIMAL(13, 3),
  PRIMARY KEY(ID, validFrom)
)