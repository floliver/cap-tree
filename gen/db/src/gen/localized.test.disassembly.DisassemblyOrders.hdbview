VIEW localized_test_disassembly_DisassemblyOrders AS SELECT
  L_0.ID,
  L_0.createdAt,
  L_0.createdBy,
  L_0.modifiedAt,
  L_0.modifiedBy,
  coalesce(localized_1.description, L_0.description) AS description,
  L_0.cutListHeader_ID,
  L_0.disassemblyProcess_ID,
  L_0.plant_Plant,
  L_0.productionOrder_ManufacturingOrder,
  L_0._productionOrder_orderType,
  L_0._productionOrder_startDate,
  L_0._productionOrder_endDate,
  L_0._productionOrder_quantity
FROM (test_disassembly_DisassemblyOrders AS L_0 LEFT JOIN test_disassembly_DisassemblyOrders_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = SESSION_CONTEXT('LOCALE'))