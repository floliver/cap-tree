VIEW localized_test_disassembly_MRPCalculationHeaders AS SELECT
  L_0.ID,
  L_0.createdAt,
  L_0.createdBy,
  L_0.modifiedAt,
  L_0.modifiedBy,
  coalesce(localized_1.description, L_0.description) AS description,
  L_0.cutListHeader_ID,
  L_0.plant_Plant,
  L_0.startDate,
  L_0.endDate
FROM (test_disassembly_MRPCalculationHeaders AS L_0 LEFT JOIN test_disassembly_MRPCalculationHeaders_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = SESSION_CONTEXT('LOCALE'))