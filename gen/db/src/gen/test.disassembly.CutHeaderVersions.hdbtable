COLUMN TABLE test_disassembly_CutHeaderVersions (
  ID NVARCHAR(36) NOT NULL,
  createdAt TIMESTAMP,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP,
  modifiedBy NVARCHAR(255),
  cutHeader_ID NVARCHAR(36),
  version INTEGER,
  cutIdentifier NVARCHAR(5000),
  description NVARCHAR(5000),
  disassemblyQuantity DECIMAL(13, 3) DEFAULT 100,
  cutType_type NVARCHAR(1),
  plantGroup_ID NVARCHAR(36),
  material_Product NVARCHAR(18),
  disassemblyProcess_ID NVARCHAR(36),
  PRIMARY KEY(ID)
)